{
  "name": "ym-sku",
  "description": "sku 数据处理插件, 为了样式的可定制化,只处理 sku 数据,UI 可自己随意绘制",
  "version": "2.0.0",
  "private": false,
  "exports": {
    ".": {
      "require": {
        "node": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "node": "./dist/index.mjs",
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./*": "./*",
    "./dist/*": "./dist/*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": {
    "name": "zhushengjie"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev:vue": "vite --config ./playground/vue/vite.config.ts ./playground/vue",
    "dev:react": "vite --config ./playground/react/vite.config.ts ./playground/react",
    "build": "tsup",
    "test": "jest --config jestconfig.json",
    "publishing": "release-it",
    "release": "npm run build && npm run publishing"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YeMao-Zi/ym-sku"
  },
  "keywords": [
    "sku",
    "hooks"
  ],
  "devDependencies": {
    "@release-it/conventional-changelog": "^9.0.0",
    "@swc/core": "^1.4.12",
    "@types/jest": "^29.5.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-vue": "^4.3.4",
    "jest": "^29.6.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "release-it": "^17.10.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.2",
    "typescript": "^5.1.6",
    "vite": "^4.2.0",
    "vue": "^3.2.47",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.15"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
